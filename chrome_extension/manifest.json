{
  "manifest_version": 3,
  "name": "StreetEasy Extractor",
  "version": "1.0",
  "permissions": ["scripting", "activeTab"],
  "host_permissions": [
    "https://streeteasy.com/*",
    "https://script.google.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
